//Forgot Password header
#ifndef FORGOTTENPASSWORD_H
#define FORGOTTENPASSWORD_H

#include <QDialog>
#include <QtSql>
#include <QDebug>
#include <QFileInfo>

namespace Ui {
class forgottenPassword;
}

class forgottenPassword : public QDialog
{
    Q_OBJECT

public:

    QSqlDatabase mydb;
    void connectionClose()
{
    mydb.close();
    mydb.removeDatabase(QSqlDatabase::defaultConnection);
}

    bool connectionOpen()
{
    mydb=QSqlDatabase::addDatabase("QSQLITE");
    mydb.setDatabaseName("C:/ChessProject/logindb.db");

    if (!mydb.open())
    {
        qDebug()<<("Database Connection Failed");
        return false;
    }

    else
    {
        qDebug()<<("Database Connection Successful");
        return true;
    }

    }


public:
    explicit forgottenPassword(QWidget *parent = 0);
    ~forgottenPassword();

private slots:
    void on_getPWButton_clicked();

private:
    Ui::forgottenPassword *ui;
};

#endif // FORGOTTENPASSWORD_H

