void multiPMode::on_fUNButton2_clicked()
{
    forgottenUserName forgottenusername;
    forgottenusername.setModal(true);
    forgottenusername.exec();
}

//the following function opens the 'Forgotten Password' page when the 'Forgotten Password' button is clicked on.
void multiPMode::on_fPWButton2_clicked()
{
    forgottenPassword forgottenpassword;
    forgottenpassword.setModal(true);
    forgottenpassword.exec();
}

//Multi Player Sign Up source code
#include "multipmode2.h"
#include "ui_multipmode2.h"
#include "multipchess.h"
#include <QSqlDatabase>
#include <QSqlQuery>
#include <QtSql>
#include <QMessageBox> //lets the program know that a messagebox will be used in this window.
#include <QStringList> //informs the program that a list of string will used.

multiPMode2::multiPMode2(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::multiPMode2)
{
    ui->setupUi(this);
    multiPMode2::showMaximized();
    this->setStyleSheet("background-color: #1d1d1d; color: white;");
    this->setWindowTitle("Multi Player Sign Up");
    ui->statusLabel5->setStyleSheet("color: white;");

    ui->pwInput3->setEchoMode(QLineEdit::Password);
    ui->pwInput4->setEchoMode(QLineEdit::Password);
    ui->answerInput2->setEchoMode(QLineEdit::Password);
    ui->answerInput3->setEchoMode(QLineEdit::Password);

    QStringList list=(QStringList()<<"Favourite Teacher"<<"Favourite Place"<<"Favourite TV Programme");
    ui->securityQuestInput2->addItems(list);
    ui->securityQuestInput3->addItems(list);


    if (!connectionOpen())
        ui->statusLabel5->setText("Database Connection Failed");
    else
        ui->statusLabel5->setText("Database Connection Successful");

}

multiPMode2::~multiPMode2()
{
    delete ui;
}

void multiPMode2::on_createAccButton4_clicked()
{
    multiPMode connection;
    QString username, password, email, securityQuestion, answer;
    username=ui->unInput3->text();
    password=ui->pwInput3->text();
    email=ui->emailInput2->text();
    securityQuestion=ui->securityQuestInput2->currentText();
    answer=ui->answerInput2->text();

    if(!connection.connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
//...Once the create account button has been clicked on by the user, the following query takes the values inputted in
//in each field and stores them in the database according to their correct database entity. For example, the
//information inputted by the user in the 'email' field will then be stored under the 'email' entity in the SQLite
//database.
    QSqlQuery qry;
    qry.prepare("INSERT INTO user (username, password, email, securityQuestion, answer) VALUES ('"+ username +"', '"+password+"','"+email+"', '"+securityQuestion+"','"+answer+"')");

    if(qry.exec())
    {
//Once all details have been entered and the 'create account'  button is clicked on, a message box willoccur, stating
//that the sign up process has been successfully completed. Following this the form fields are updated and thus cleared.
        QMessageBox::information(this, tr("Create Account"), tr("Your Account Has Successfully Been Created"));
        connection.connectionClose();

        foreach(QLineEdit* tr, findChildren<QLineEdit*>())
        {
            tr->clear();
        }
    }
    else
    {
//Nevertheless, if the sign up fails, an error meaage box will then appear stating what exactly the error is;
//I created this using the '.lastError()' keyword.
        QMessageBox::critical(this, tr("ERROR 404!"), qry.lastError().text());
    }
}

void multiPMode2::on_createButton5_clicked()
{
    multiPMode connection;
    QString username, password, email, securityQuestion, answer;

    username=ui->unInput4->text();
    password=ui->pwInput4->text();
    email=ui->emailInput3->text();
    securityQuestion=ui->securityQuestInput3->currentText();
    answer=ui->answerInput3->text();

    if(!connection.connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
    QSqlQuery qry;
    qry.prepare("INSERT INTO user (username, password, email, securityQuestion, answer) VALUES ('"+ username +"', '"+password+"','"+email+"', '"+securityQuestion+"','"+answer+"')");

    if(qry.exec())
    {
        QMessageBox::information(this, tr("Create Account"), tr("Your Account Has Successfully Been Created"));
        connection.connectionClose();

        foreach(QLineEdit* tr, findChildren<QLineEdit*>())
        {
            tr->clear();
        }
    }
    else
    {
        QMessageBox::critical(this, tr("ERROR 404!"), qry.lastError().text());
    }

}

//open the multi player mode of the chess game.
void multiPMode2::on_createAccButton2_clicked()
{
    multiPChess multipchess;
    multipchess.setModal(true);
    multipchess.exec();
}

