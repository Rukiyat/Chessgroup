    instructions.setModal(true);
    instructions.exec();
}

//Single Player Login source code
#include "singlepmode.h"
#include "ui_singlepmode.h"
#include "forgottenusername.h"
#include "forgottenpassword.h"
#include "singlepchess.h"
#include <QSqlDatabase>
#include <QSqlQuery>
#include <QtSql>
#include <QMessageBox>



singlePMode::singlePMode(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::singlePMode)
{
    ui->setupUi(this);
    singlePMode::showMaximized();
    this->setStyleSheet("background-color: #1d1d1d; color: white;");
    this->setWindowTitle("Single Player Login");

    ui->pwInput1->setEchoMode(QLineEdit::Password);
    ui->pwInput2->setEchoMode(QLineEdit::Password);
    ui->statusLabel->setStyleSheet("color: white;");
    QStringList list=(QStringList()<<"Favourite Teacher"<<"Favourite Place"<<"Favourite TV Programme");
    ui->spmsqListBox->addItems(list);


    if (!connectionOpen())
        ui->statusLabel->setText("Database Connection Failed");
    else
        ui->statusLabel->setText("Database Connection Successful");

}

singlePMode::~singlePMode()
{
    delete ui;
}

void singlePMode::on_fUNButton_clicked()
{
   forgottenUserName forgottenusername;
   forgottenusername.setModal(true);
   forgottenusername.exec();
}

void singlePMode::on_fPWButton_clicked()
{
    forgottenPassword forgottenpassword;
    forgottenpassword.setModal(true);
    forgottenpassword.exec();
}

//
void singlePMode::on_loginButton_clicked()
{
    QString username, password;
    username=ui->unInput1->text();
    password=ui->pwInput1->text();

    if(!connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
    QSqlQuery qry;
    qry.prepare("select * from user where username = '"+ username +"' and password = '"+ password +"'");

    if(qry.exec())
    {
        int count = 0; //counts number of times it executes the query
        while(qry.next())
        {
            count++; //increments the query by 1
        }
        if(count ==1)
        {
            ui->statusLabel->setText("Login Successful!");
            connectionClose();
            this->hide();
//opens the single player mode of the chess game.
            singlePChess singlepchess;
            singlepchess.setModal(true);
            singlepchess.exec();
        }

        if(count <1)
            ui->statusLabel->setText("Login Unsuccessful! \nPlease Check That Your Username & Password are Correct.");
    }
}

void singlePMode::on_createAccButton_clicked()
{
    singlePMode connection;
    QString username, password, email, securityQuestion, answer;
    username=ui->unInput2->text();
    password=ui->pwInput2->text();
    email=ui->emailInput->text();
    securityQuestion=ui->spmsqListBox->currentText();
    answer=ui->answerInput->text();

    if(!connection.connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
    QSqlQuery qry;
    qry.prepare("INSERT INTO user (username, password, email, securityQuestion, answer) VALUES ('"+ username +"', '"+password+"','"+email+"', '"+securityQuestion+"','"+answer+"')");

    if(qry.exec())
    {
        QMessageBox::information(this, tr("Create Account"), tr("Your Account Has Successfully Been Created"));
        connection.connectionClose();

        foreach(QLineEdit* tr, findChildren<QLineEdit*>())
        {
            tr->clear();
        }
    }
    else
    {
        QMessageBox::critical(this, tr("ERROR 404!"), qry.lastError().text());
    }
}

//Single Player Chess Game window source code. (Excludes the chess board's source code - attempted by other members of the group).
#include "singlepchess.h"
#include "ui_singlepchess.h"

singlePChess::singlePChess(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::singlePChess)
{
    ui->setupUi(this);
    singlePChess::showMaximized();
    this->setWindowTitle("Chess 2.0: Single Player Mode");
    this->setStyleSheet("background-color: #1d1d1d;");
    ui->statusLabel2->setStyleSheet("color: white;");

    singlePMode connection;
    if(!connection.connectionOpen())
        ui->statusLabel2->setText("Database Connection Failed");
    else
        ui->statusLabel2->setText("Database Connection Successful");

}

singlePChess::~singlePChess()
{
    delete ui;
}

//Multi Player Login source code
#include "multipmode.h" //passing through the 'Multi Player Sign Up' window.
#include "ui_multipmode.h" //passing through the 'Multi Player Sign Up' useer interface.
#include "multipchess.h" //passing through the 'Multi Player Mode' header.
#include "multipmode2.h" //passing through the 'Multi Player Login' header.
#include "forgottenusername.h" //passing through the 'Forgot Username' header.
#include "forgottenpassword.h" //passing through the 'Forgot Password' header.
#include <QSqlDatabase> //informs the program that a database creted in SQLite will be connected to this form.
#include <QSqlQuery> //lets the program know that SQLite queries will be performed in this form.
#include <QtSql> //lets the program know that SQLite will be incorporated.

//assigns the window to its form.
multiPMode::multiPMode(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::multiPMode)
{
    ui->setupUi(this);
    multiPMode::showMaximized(); //opens the page in fullscreen mode.
    this->setStyleSheet("background-color: #1d1d1d; color: white;"); //sets thebackground & font colour of the window.
    this->setWindowTitle("Multi Player Login"); //sets the window bar title.
    ui->statusLabel3->setStyleSheet("color: white;"); //sets the font colour of the database status.

//hides the text inputted by the user when typing in their password.
    ui->pwInputp1->setEchoMode(QLineEdit::Password);
    ui->pwInputp2->setEchoMode(QLineEdit::Password);

//"creates a pathway for the database to be opened; if the database succesfully connects with the gui, then it displays
// it executes the second condition. If not then the first condition is executed.
    if (!connectionOpen())
        ui->statusLabel3->setText("Database Connection Failed");
    else
        ui->statusLabel3->setText("Database Connection Successful");
}
//this function deletes the window when the 'X' button is clicked on.
multiPMode::~multiPMode()
{
    delete ui;
}
//this function checks that the details inputted by the user upon logging matches a record stored in the database...
void multiPMode::on_loginButton2_clicked()
{
    QString username, password;
    username=ui->unInputp1->text();
    username=ui->unInputp2->text();
    password=ui->pwInputp1->text();
    password=ui->pwInputp2->text();

    if(!connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
    QSqlQuery qry;
    qry.prepare("select * from user where username = '"+ username +"' and password = '"+ password +"'");

//...Once the login button has been clicked on by the user, the following condition checks to see whether the inputted
//details match a record in the SQLite database. If there is a singular match, the first condition of the 'nested if
//statement' is executed; however if the details are incorrect or non-existent, then the program updates its status by
//informing the user that the login has been unsuccessful.

    if(qry.exec())
    {
        int count = 0; //counts number of times it executes the query
        while(qry.next())
        {
            count++;
        }
        if(count ==1)
        {
//if the login issuccessful then the program opens the window containing the chess board.
            ui->statusLabel3->setText("Login Successful!");
            connectionClose();
            this->hide();
            multiPChess multipchess;
            multipchess.setModal(true);
            multipchess.exec();
        }

        if(count <1)
            ui->statusLabel3->setText("Login Unsuccessful! \nPlease Check That Your Username & Password are Correct.");
    }
}

//the following function opens the 'Multi Player Sign In' page when the 'Sign Up' button is selected by the user.
void multiPMode::on_signupButton_clicked()
{
    multiPMode2 multipmode2;
    multipmode2.setModal(true);
    multipmode2.exec();
}

