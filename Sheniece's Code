//Forgotten Username source code
#include "forgottenusername.h"
#include "ui_forgottenusername.h"
#include <QSqlDatabase>
#include <QSqlQuery>
#include <QtSql>

forgottenUserName::forgottenUserName(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::forgottenUserName)
{
    ui->setupUi(this);
    forgottenUserName::showNormal();
    this->setStyleSheet("background-color: white;");
    this->setWindowTitle("Forgotten Username?");

//loads the three following items into the 'Security Question' combo box, so that the user can choose their preferred
//security question upon signing up.
    QStringList list=(QStringList()<<"Favourite Teacher"<<"Favourite Place"<<"Favourite TV Programme");
    ui->fUNSecurityInput->addItems(list);
}

forgottenUserName::~forgottenUserName()
{
    delete ui;
}

//the function below takes the information inputted by the user and performs a 'VLOOKUP' function using a query in the
//SQLite database, in order to return the desired value, being the user's username.
void forgottenUserName::on_getUNButton_clicked()
{
    forgottenUserName connection;
    QString email, securityQuestion, answer;
    email=ui->fUNInput->text();
    securityQuestion=ui->fUNSecurityInput->currentText();
    answer=ui->fUNAnswerInput->text();

    QSqlQueryModel *  username=new QSqlQueryModel();

    if(!connection.connectionOpen())
    {
//if the connection between the SQLite database and the gui fails, then the following message is returned by the program.
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen(); //establish a connection between the database and the query.
    QSqlQuery* qry = new QSqlQuery(connection.mydb);
    qry->prepare("SELECT username FROM user WHERE email = '"+ email +"' and securityQuestion = '"+ securityQuestion +"'and answer = '"+ answer +"'");
    qry->exec();

//when the program locates the desired value, the valueis then passed to a list view box, where the username is then
//dispalyed for the user to see.
    username->setQuery((*qry));
    ui->revealUsername->setModel(username);

    connectionClose(); //closes the database connection.
}

//Forgotten Password source code
#include "forgottenpassword.h"
#include "ui_forgottenpassword.h"
#include <QSqlDatabase>
#include <QSqlQuery>
#include <QtSql>

forgottenPassword::forgottenPassword(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::forgottenPassword)
{
    ui->setupUi(this);
    forgottenPassword::showNormal();
    this->setStyleSheet("background-color: white;");
    this->setWindowTitle("Forgotten Password?");
    QStringList list=(QStringList()<<"Favourite Teacher"<<"Favourite Place"<<"Favourite TV Programme");
    ui->fpSecurityQuestInput->addItems(list);
}

forgottenPassword::~forgottenPassword()
{
    delete ui;
}

//the function below takes the information inputted by the user and performs a 'VLOOKUP' function using a query in the
//SQLite database, in order to return the desired value, being the user's password.
void forgottenPassword::on_getPWButton_clicked()
{
    forgottenPassword connection;
    QString email, securityQuestion, answer;
    email=ui->fpEmailInput->text();
    securityQuestion=ui->fpSecurityQuestInput->currentText();
    answer=ui->fpAnswerInput->text();

    QSqlQueryModel *  password=new QSqlQueryModel();

    if(!connection.connectionOpen())
    {
        qDebug()<<"Failed to open Database";
        return;
    }
    connectionOpen();
    QSqlQuery* qry = new QSqlQuery(connection.mydb);
    qry->prepare("SELECT password FROM user WHERE email = '"+ email +"' and securityQuestion = '"+ securityQuestion +"'and answer = '"+ answer +"'");
    qry->exec();
//when the program locates the desired value, the value is then passed to a list view box, where the password is then
//revealed to the user.
    password->setQuery((*qry));
    ui->revealPassword->setModel(password);

    connectionClose();

}

//Instructions window source code. (Excludes actual instuctions - created by a different member of the group).
#include "instructions.h"
#include "ui_instructions.h"

Instructions::Instructions(QWidget *parent) :
    QDialog(parent),
    ui(new Ui::Instructions)
{
    ui->setupUi(this);
    Instructions::showMaximized();
    this->setStyleSheet("background-color: #1d1d1d;");
    this->setWindowTitle("Instructions");
}

Instructions::~Instructions()
{
    delete ui;
}


